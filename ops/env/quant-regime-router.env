# Regime router worker (opt-in).
# Keeps V2 control path unchanged and only updates strategy_control entry flags
# for explicitly managed strategies.
REGIME_ROUTER_ENABLED=0
REGIME_ROUTER_POLL_SEC=5
REGIME_ROUTER_HEARTBEAT_SEC=60
REGIME_ROUTER_MIN_DWELL_SEC=30
REGIME_ROUTER_MACRO_TF=H4
REGIME_ROUTER_MICRO_TF=M1
REGIME_ROUTER_EVENT_MODE=0
REGIME_ROUTER_SNAPSHOT_PATH=logs/regime_router_state.json

# Managed strategy slugs (normalized in strategy_control).
# D narrow 運用では router 有効時も C/B/flow へ戻さない。
REGIME_ROUTER_MANAGED_STRATEGIES=scalp_ping_5s_d

# Route target strategies by macro/micro regime.
REGIME_ROUTER_TREND_ENTRY_STRATEGIES=scalp_ping_5s_d
REGIME_ROUTER_BREAKOUT_ENTRY_STRATEGIES=scalp_ping_5s_d
REGIME_ROUTER_RANGE_ENTRY_STRATEGIES=scalp_ping_5s_d
REGIME_ROUTER_MIXED_ENTRY_STRATEGIES=scalp_ping_5s_d
REGIME_ROUTER_EVENT_ENTRY_STRATEGIES=scalp_ping_5s_d
REGIME_ROUTER_UNKNOWN_ENTRY_STRATEGIES=scalp_ping_5s_d
