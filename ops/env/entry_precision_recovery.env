# Entry precision recovery (2026-02-11)
# Goal: keep trading active while reducing clustered stop-loss bursts.

# Do not bypass stale-factor gating for fast pockets.
ENTRY_FACTOR_MAX_AGE_SEC=300
ENTRY_FACTOR_STALE_ALLOW_POCKETS=manual,macro

# Re-enable counter-trend block on M1 scalp entries.
M1SCALP_HTF_BLOCK_COUNTER=1
M1SCALP_COOLDOWN_SEC=20

# Micro multi: reduce burst density and require fresher factors.
MICRO_MULTI_MAX_SIGNALS_PER_CYCLE=1
MICRO_MULTI_STRATEGY_COOLDOWN_SEC=45
MICRO_MULTI_MAX_FACTOR_AGE_SEC=180
MICRO_MULTI_LOOP_INTERVAL_SEC=6.0

# Keep trend-flip restrictions explicit for mean-reversion tags.
MICRO_MULTI_TREND_FLIP_STRATEGY_BLOCKLIST=MicroLevelReactor,MicroCompressionRevert
