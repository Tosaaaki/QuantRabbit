# Dedicated env for quant-scalp-failed-break-reverse.service
# Disabled by default to avoid conflict with existing scalp workers.
M1SCALP_ENABLED=1
M1SCALP_SIGNAL_TAG_CONTAINS=vshape-rebound
M1SCALP_STRATEGY_TAG_OVERRIDE=FailedBreakReverse
M1SCALP_ALLOW_REVERSION=1
M1SCALP_ALLOW_TREND=0
M1SCALP_SIDE_FILTER=none
M1SCALP_MAX_SPREAD_PIPS=1.00
spread_guard_max_pips=1.00
M1SCALP_USE_LIMIT_ENTRY=1
