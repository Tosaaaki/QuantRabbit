[Unit]
Description=QuantRabbit FastScalp Exit Worker
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/tossaki/QuantRabbit
EnvironmentFile=-/etc/quantrabbit.env
Environment="PYTHONUNBUFFERED=1"
Environment=FAST_SCALP_POCKET=scalp
ExecStart=/home/tossaki/QuantRabbit/.venv/bin/python -m workers.fast_scalp.exit_worker
Restart=always
RestartSec=5
User=tossaki

[Install]
WantedBy=multi-user.target
