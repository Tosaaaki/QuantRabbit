[Unit]
Description=QuantRabbit Pullback Scalp Worker
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/tossaki/QuantRabbit
Environment="PYTHONUNBUFFERED=1"
ExecStart=/home/tossaki/QuantRabbit/.venv/bin/python -m workers.pullback_scalp.worker
Restart=always
RestartSec=5
User=tossaki

[Install]
WantedBy=multi-user.target
