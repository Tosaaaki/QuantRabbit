[Unit]
Description=QuantRabbit Mirror Spike S5 Worker
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/tossaki/QuantRabbit
Environment="PYTHONUNBUFFERED=1"
ExecStart=/home/tossaki/QuantRabbit/.venv/bin/python -m workers.mirror_spike_s5.worker
Restart=always
RestartSec=5
User=tossaki

[Install]
WantedBy=multi-user.target
