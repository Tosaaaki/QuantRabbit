[Unit]
Description=QuantRabbit type maintenance (local validation)
After=network-online.target

[Service]
Type=oneshot
User=tossaki
WorkingDirectory=/home/tossaki/QuantRabbit
Environment=PYTHONUNBUFFERED=1
Environment=TYPE_MAINTENANCE_MODE=check
ExecStart=/home/tossaki/QuantRabbit/scripts/type_maintenance_vm.sh
