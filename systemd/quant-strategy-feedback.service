[Unit]
Description=QuantRabbit strategy feedback worker (analysis tuning hints)
After=network-online.target

[Service]
Type=oneshot
WorkingDirectory=/home/tossaki/QuantRabbit
EnvironmentFile=-/home/tossaki/QuantRabbit/ops/env/quant-v2-runtime.env
EnvironmentFile=-/home/tossaki/QuantRabbit/ops/env/quant-strategy-feedback.env
Environment=PYTHONUNBUFFERED=1
ExecStart=/home/tossaki/QuantRabbit/.venv/bin/python -m analysis.strategy_feedback_worker
User=tossaki

[Install]
WantedBy=multi-user.target
