[Unit]
Description=QuantRabbit ping5s-d canary guard worker
After=network-online.target

[Service]
Type=oneshot
WorkingDirectory=/home/tossaki/QuantRabbit
Environment=PYTHONUNBUFFERED=1
EnvironmentFile=-/home/tossaki/QuantRabbit/ops/env/quant-v2-runtime.env
EnvironmentFile=-/home/tossaki/QuantRabbit/ops/env/quant-ping5s-d-canary-guard.env
ExecStart=/bin/bash /home/tossaki/QuantRabbit/scripts/run_ping5s_d_canary_guard.sh
User=tossaki

[Install]
WantedBy=multi-user.target

