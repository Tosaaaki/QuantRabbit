[Unit]
Description=QuantRabbit trade counterfactual review worker
After=network-online.target

[Service]
Type=oneshot
WorkingDirectory=/home/tossaki/QuantRabbit
Environment=PYTHONUNBUFFERED=1
EnvironmentFile=-/home/tossaki/QuantRabbit/ops/env/quant-v2-runtime.env
EnvironmentFile=-/home/tossaki/QuantRabbit/ops/env/quant-trade-counterfactual.env
ExecStart=/home/tossaki/QuantRabbit/.venv/bin/python -m analysis.trade_counterfactual_worker
User=tossaki

[Install]
WantedBy=multi-user.target
